config system
	option hostname	XiaoQiang
	option timezone	UTC
	option log_size	64

config hostdns 'netdt'
	option cn_domain 'www.google.com www.facebook.com'
	option cn_dns '1.1.1.1 1.0.0.1'
	option world_domain 'www.google.com www.facebook.com'
	option world_dns '1.1.1.1 1.0.0.1'
		
config timeserver ntp
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'
	option enabled 1
	option enable_server 0
